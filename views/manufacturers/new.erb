<h3>New Entry</h3>

<form method="post" action="/manufacturers">
    <label for="name-input">Manufacturer Name: </label>
        <input id="name-input" type="text" name="name"/>

    <label for="hq-input">Manufacturer Hq: </label>
        <input id="hq-input" type="text" name="hq"/>

    <label for="tel_no-input">Manufacturer Telephone Number: </label>
        <input id="tel_no-input" type="text" name="tel_no"/>

    <input class="btn" type="submit" value="Add Manufacturer"/>
</form>
<hr/>
 <div class="new-entry-div">
        <form method="get" action="/manufacturers">
            <input class="btn btn-show" type="submit" value="Back To Inventory"/>
        </form>
    </div>